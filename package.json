{
	"name": "control-unity",
	"version": "0.0.1",
	"author": {
		"name": "Stiwar Asprilla & Jhon E. Palacios"
	},
	"displayName": "Control Unity | Sistema de control de implementos y salones",
	"description": "The objective of Control Unity is to have control of the inventory of implements and the reservations of the classrooms of the educational complex",
	"keywords": [
		"bookings",
		"implements",
		"classrooms",
		"university"
	],
	"private": true,
	"scripts": {
		"dev": "vite",
		"build": "run-p type-check \"build-only {@}\" --",
		"preview": "vite preview",
		"test:unit": "vitest",
		"build-only": "vite build",
		"type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
		"generate-pwa-assets": "pwa-assets-generator --preset minimal public/images/LOGO_PRINCIPAL_CU.svg",
		"purify-css": "purifycss ./src/**/*.vue ./src/**/*.ts ./src/**/*.css --min --out ./dist/purified.css"
	},
	"dependencies": {
		"@supabase/supabase-js": "^2.38.3",
		"file-saver": "^2.0.5",
		"pinia": "^2.1.7",
		"vue": "^3.3.4",
		"vue-router": "^4.2.5",
		"vuetify": "^3.3.23",
		"xlsx": "^0.18.5"
	},
	"devDependencies": {
		"@mdi/font": "^7.3.67",
		"@tsconfig/node18": "^18.2.2",
		"@types/file-saver": "^2.0.6",
		"@types/jsdom": "^21.1.3",
		"@types/node": "^20.8.10",
		"@vite-pwa/assets-generator": "^0.0.11",
		"@vitejs/plugin-vue": "^4.4.0",
		"@vue/test-utils": "^2.4.1",
		"@vue/tsconfig": "^0.4.0",
		"jsdom": "^22.1.0",
		"npm-run-all2": "^6.1.1",
		"typescript": "~5.2.0",
		"vite": "^4.4.11",
		"vite-plugin-compression2": "^0.11.0",
		"vite-plugin-pwa": "^0.17.2",
		"vitest": "^0.34.6",
		"vue-tsc": "^1.8.19"
	}
}
